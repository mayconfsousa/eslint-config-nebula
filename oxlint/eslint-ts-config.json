      2
    ],
    "prettier/prettier": [
      2,
      {
        "printWidth": 120,
        "tabWidth": 2,
        "singleQuote": true,
        "semi": false,
        "trailingComma": "all"
      }
    ],
    "unused-imports/no-unused-imports": [
      2
    ],
    "unused-imports/no-unused-vars": [
      2,
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ],
    "perfectionist/sort-exports": [
      2,
      {
        "type": "natural",
        "order": "asc"
      }
    ],
    "perfectionist/sort-imports": [
      2,
      {
        "type": "natural",
        "order": "asc",
        "groups": [
          "side-effect",
          "builtin",
          "react",
          "type",
          "external",
          "internal-type",
          "internal",
          [
            "parent-type",
            "sibling-type",
            "index-type"
          ],
          [
            "parent",
            "sibling",
            "index"
          ],
          "style",
          "object",
          "unknown"
        ],
        "newlinesBetween": "always",
        "internalPattern": [
          "^@/.*",
          "^~/.*"
        ],
        "customGroups": {
          "value": {
            "react": [
              "^react$",
              "^react-dom.*"
            ]
          },
          "type": {
            "react": [
              "^react$",
              "^react-dom.*"
            ]
          }
        }
      }
    ],
    "perfectionist/sort-named-exports": [
      2,
      {
        "type": "natural",
        "order": "asc",
        "groupKind": "values-first"
      }
    ],
    "perfectionist/sort-named-imports": [
      2,
      {
        "type": "natural",
        "order": "asc",
        "groupKind": "values-first"
      }
    ]
  },
  "plugins": [
    "@",
    "@typescript-eslint:@typescript-eslint/eslint-plugin@8.28.0",
    "prettier:eslint-plugin-prettier@5.2.1",
    "unused-imports:unused-imports",
    "perfectionist:perfectionist"
  ],
  "language": "@/js",
  "languageOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest",
    "parser": "typescript-eslint/parser@8.28.0",
    "parserOptions": {
      "projectService": true,
      "tsconfigRootDir": "/root/dev/eslint-config-nebula/src"
    }
  }
}
